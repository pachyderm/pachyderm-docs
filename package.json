{
  "private": true,
  "name": "pachyderm-docs",
  "description": "Docs Starter built with Remix and powered by Hygraph",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "dev": "node -r dotenv/config node_modules/.bin/concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "generate": "graphql-codegen -r dotenv/config --config codegen.yml",
    "format": "prettier --ignore-path .gitignore --write . --no-error-on-unmatched-pattern"
  },
  "dependencies": {
    "@graphcms/rich-text-react-renderer": "^0.6.1",
    "@graphcms/rich-text-types": "^0.5.0",
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@netlify/functions": "^1.0.0",
    "@remix-run/netlify": "^1.6.6",
    "@remix-run/node": "^1.6.6",
    "@remix-run/react": "^1.6.6",
    "cross-env": "^7.0.3",
    "classcat": "^5.0.3",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.6.6",
    "remix-utils": "^2.11.1",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.11.2",
    "@graphql-codegen/fragment-matcher": "^3.3.0",
    "@graphql-codegen/typescript": "^2.7.2",
    "@graphql-codegen/typescript-graphql-request": "^4.5.2",
    "@graphql-codegen/typescript-operations": "^2.5.2",
    "@remix-run/dev": "^1.6.6",
    "@remix-run/eslint-config": "^1.6.6",
    "@remix-run/serve": "^1.6.6",
    "@tailwindcss/typography": "^0.5.4",
    "@types/react": "^17.0.48",
    "@types/react-dom": "^17.0.17",
    "@types/url-join": "^4.0.1",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "dotenv": "^14.3.2",
    "eslint": "^8.20.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.7",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
